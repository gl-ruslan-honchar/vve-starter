{
  "name": "vve-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite --open --host --port 4400",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:prod:ssg": "cross-env NODE_ENV=production vite-ssg build",
    "build:preview": "vite preview",
    "lint": "pnpm lint:types && pnpm lint:css && pnpm lint:js && pnpm lint:imports && pnpm lint:prettier",
    "lint:types": "vue-tsc --noEmit",
    "lint:css": "stylelint --fix \"**/*.{css,scss,vue}\"",
    "lint:js": "eslint --fix --ext \"**/*.{js,ts,vue}\"",
    "lint:imports": "import-sort --write \"src/**/*{.js,.ts,.vue}\"",
    "lint:prettier": "prettier --write --loglevel error \"**/*.{js,ts,json,css,scss,vue,html}\"",
    "test": "pnpm test:unit:run",
    "test:unit:ui": "vitest --ui",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest --coverage",
    "test:e2e": "cypress open",
    "test:ci:e2e": "cypress run --headless",
    "audit": "run-s audit:*",
    "audit:dev": "pnpm audit --json --dev",
    "audit:prod": "pnpm audit --json --prod",
    "audit:force:fix": "pnpm audit --fix",
    "analyze:windicss": "npx windicss-analysis",
    "release": "standard-version",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky install && chmod ug+x .husky/*"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "@vueuse/head": "^0.7.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.0",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@iconify-json/carbon": "^1.0.12",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@pinia/testing": "^0.0.9",
    "@types/node": "^17.0.8",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitest/ui": "^0.1.12",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "c8": "^7.11.0",
    "commitizen": "^4.2.4",
    "critters": "^0.0.16",
    "cypress": "^9.2.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "happy-dom": "^2.25.2",
    "husky": "^7.0.4",
    "imagemin-lint-staged": "^0.4.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-config": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-eslint": "^6.0.0",
    "lint-staged": "^12.1.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "sass": "^1.48.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.2.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.5.10",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.11",
    "vite": "^2.7.2",
    "vite-plugin-pages": "^0.20.0",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.6.2",
    "vite-ssg": "^0.17.6",
    "vitest": "^0.1.12",
    "vue-tsc": "^0.30.2",
    "windicss": "^3.4.2"
  },
  "engines": {
    "node": ">=14.17.0",
    "pnpm": ">=6.21.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
